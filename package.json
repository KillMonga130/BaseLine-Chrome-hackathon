{
  "name": "baseline-tooling-starter",
  "version": "0.1.0",
  "description": "Starter scaffold for the Baseline Tooling Hackathon: small CLI + submission materials",
  "main": "src/index.js",
  "bin": {
    "baseline": "src/index.js"
  },
  "scripts": {
  "start": "node src/index.js",
  "demo": "node src/demo.js",
  "test": "node test/run.js",
  "lint-css": "node tools/lint-css.js example/styles.css",
  "test:rule": "node test/lint-rule-test.js",
    "lint:baseline": "npx eslint -c .eslintrc-baseline.cjs example/styles.js -f stylish",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "baseline",
    "web-features",
    "hackathon",
    "eslint",
    "tooling"
  ],
  "license": "MIT",
  "dependencies": {
    "eslint": "^9.37.0",
    "eslint-plugin-baseline": "file:eslint-plugin-baseline",
    "node-fetch": "^2.7.0",
    "web-features": "^1.4.0"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  }
}
